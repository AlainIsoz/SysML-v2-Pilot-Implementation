		g.self,  test.g.self,  h.self,  test.h.self
		--- */
	class B conjugates A;

	//XPECT linkedName at B::f --> test.A.f	
	feature g ~ B::f;
	//XPECT linkedName at B::f_Id --> test.A.f
	feature h ~ B::f_Id;
