// XPECT noErrors ---> ""
package Example2 {
	block Fuel;
	port def FuelOutPort {
		ref out fuelSupply : Fuel;
		ref in fuelReturn : Fuel;
	}
	block FuelTankAssembly {
		port fuelTankPort : FuelOutPort;
	}

	block Vehicle;
	block FuelPump;
	block FuelTank;
	
	part vehicle : Vehicle {	
		part tank : FuelTankAssembly {
			port fuelTankPort0 redefines FuelTankAssembly::fuelTankPort {
				ref out fuelSupply redefines FuelTankAssembly::fuelTankPort::fuelSupply;
				ref in redefines fuelReturn;
			}
			port redefines FuelTankAssembly::fuelTankPort {
				ref out fuelSupply redefines FuelTankAssembly::fuelTankPort::fuelSupply;
			}
			
			bind fuelTankPort0::fuelSupply = pump::pumpOut;
//short-hand redefines syntax not supported yet that causes errors
			// XPECT errors --> "Invalid Connector - Type of connector and its relatedFeatures do not match" at "fuelTankPort0::fuelReturn = tank::fuelIn;"
			bind fuelTankPort0::fuelReturn = tank::fuelIn;
			// XPECT errors --> "Invalid Connector - Type of connector and its relatedFeatures do not match" at "fuelTankPort::fuelSupply = pump::pumpOut;"
			bind fuelTankPort::fuelSupply = pump::pumpOut;
					
			part pump : FuelPump {
				ref out pumpOut : Fuel;
				ref in pumpIn : Fuel;
			}
			
			part tank : FuelTank {
				ref in fuelOut : Fuel;
				ref in fuelIn : Fuel;
			}
		}
	} 
}