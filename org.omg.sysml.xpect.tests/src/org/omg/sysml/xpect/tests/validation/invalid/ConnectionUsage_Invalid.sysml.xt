package P {
	block ABlock;	block B;	block C;	block D;	block E;
	
	assoc block AB {
		end : D[1];
		end : E[1];
	}
	assoc block BC {
		end : B[1];
		end : C[1];
	}
	part apart : ABlock {
		part b : B[1];
		part c: C[1];
		
		link ab : AB connect b to c;
		link bc : AB, BC connect b to c;
		
		// XPECT errors --> "A connection must be typed by connection definitions." at ":ABlock connect b to c;"
		link :ABlock connect b to c;
		// XPECT errors --> "A connection must be typed by connection definitions." at ":apart connect b to c;"
		link :apart connect b to c;
	}
	
	
