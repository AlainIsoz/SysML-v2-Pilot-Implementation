package Example3 {
	block Fuel;
	port def FuelOutPort {
		ref out fuelSupply : Fuel;
		ref in fuelReturn : Fuel;
	}
	block FuelTankAssembly {
		port fuelTankPort : FuelOutPort;
	}

	block Vehicle;
	part vehicle : Vehicle {
		action myAction(
			in myIn, 
			out myOut
		);
		
		part tankAssy : FuelTankAssembly {
			port fuelTankPort {
				ref out fuelSupplyOut;
				ref in fuelReturnIn;
			}
		}
		// XPECT errors --> "Output feature must conform to input feature" at "myAction::myOut = tankAssy::fuelTankPort::fuelReturnIn;"
		bind myAction::myOut = tankAssy::fuelTankPort::fuelReturnIn; //InTypes[Part, Object], OutTypes[Anything]
        bind tankAssy::fuelTankPort::fuelSupplyOut = myAction::myIn; //InTypes [Anything], OutTypes [Part, Object]
        
        
        part tankAssy2 : FuelTankAssembly {
			port fuelTankPort redefines FuelTankAssembly::fuelTankPort {
				ref out fuelSupplyOut redefines FuelTankAssembly::fuelTankPort::fuelSupply;
				ref in fuelReturnIn redefines FuelTankAssembly::fuelTankPort::fuelReturn;
			}
		}
		// XPECT errors --> "Output feature must conform to input feature" at "myAction::myOut = tankAssy2::fuelTankPort::fuelReturnIn;"
		bind myAction::myOut = tankAssy2::fuelTankPort::fuelReturnIn; //InTypes[Fuel, Part, Object], OutTypes[Anything]
        bind tankAssy2::fuelTankPort::fuelSupplyOut = myAction::myIn; //InTypes [Anything], OutTypes [Part, Object]
		
	} 
}
