package 'Redefinition Example' {

	block Engine {
		part cyl : Cylinder[4..6];
	}
	block SmallEngine :> Engine {
		// XPECT errors --> "Subsetting/redefining feature cannot have larger multiplicity upper bound" at "cyl"
		part redefines cyl[7];
		//* XPECT errors ---
 		"Redefining feature cannot have smaller multiplicity lower bound" at "cyl"
		"Subsetting/redefining feature cannot have larger multiplicity upper bound" at "cyl"
		--- */
		part mycyl: Cylinder[1..8] redefines cyl;
	}
	block BigEngine :> Engine {
		// XPECT errors --> "Redefining feature cannot have smaller multiplicity lower bound" at "cyl"
		part redefines cyl[1..6];
		part bcyl: Cylinder[*];
	}

	block Cylinder;
}
