package BindingConnectorExample {
	part def B0;
	part def B1; 
	part def V;
	part v : V {
		part b0 : B0 {
			port p0 {
				in ref myIn;
			}
		}
		part b1 : B1 {
			port p1 {
				out ref myOut;
			}
		}
		//* XPECT errors --- 
		"Relationships must have at least two related elements" at "bind b0.p0.myIn to"
		"mismatched input 'to' expecting '='" at "to"
		"no viable alternative at input '.'" at "b0"
		"no viable alternative at input '.'" at "p1"
		--- */
		bind b0.p0.myIn to b0.p1.myOut;	
	} 
}