package Import_Private {
	package P1 {
		class A;
	}
	package P2 {
		private import P1::*;
	}
	/* XPECT scope at P1::A ---
	   x, y, y1, P1.A, Import_Private.x, Import_Private.y, Import_Private.y1, Import_Private.P1.A, 
	   Base.Anything, Base.BehaviorOccurrence, Base.BinaryLink,	   Base.BinaryLink.linkage, Base.BinaryLink.participant, Base.BinaryLink.sourceEnd,	   Base.BinaryLink.sourceParticipant, Base.BinaryLink.targetEnd, Base.BinaryLink.targetParticipant,	   Base.EnumeratedValue, Base.FunctionEvaluation, Base.Link, Base.Link.participant,	   Base.LiteralEvaluation, Base.NullEvaluation, Base.Object, Base.Occurrence, Base.SelfLink,	   Base.SelfLink.myself, Base.SelfLink.self, Base.SelfLink.sourceParticipant,	   Base.SelfLink.targetParticipant, Base.Value, Base.behaviorOccurrences, Base.binaryLinks,	   Base.functionEvaluations, Base.links, Base.literalEvaluations, Base.nullEvaluations, Base.objects,	   Base.selfLinks, Base.things, Base.values
	--- */
	x: P1::A;
	
	import P2::*;
	// This should fail.
	// A is not visible, because the import in P2 is private.
	// XPECT errors --> "Couldn't resolve reference to Category 'A'." at "A"
	y: A;
	//XPECT errors --> "Couldn't resolve reference to Category 'P2::A'." at "P2::A"
	y1: P2::A;
}