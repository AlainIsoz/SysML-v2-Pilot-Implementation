package ConjugationTest {
	port def P;
	
	block B {
		port p1: P;
		port p2: ~P;
	}
	
	assoc block A {
		end port p1: P;
		end port p2: ~P;
	}
	
	interface def I {
		end p1: P;
		end p2: ~P;
	}
	
	block B1 {
		part p {
			port p1: P;
			port p2: ~P;		
		}
		//Automatic redefinition bug - added ports (p3 and p4) should not be considered as related feature.
		link a: A connect p::p1 to p::p2 {
			end port p3: P;
			end port p4: ~P;
		}
		//Automatic redefinition bug - added ports (p3 and p4) should not be considered as related feature.
		interface i: I connect p::p1 to p::p2 {
			end port p3: P;
			end port p4: ~P;
		}
	}
	
}