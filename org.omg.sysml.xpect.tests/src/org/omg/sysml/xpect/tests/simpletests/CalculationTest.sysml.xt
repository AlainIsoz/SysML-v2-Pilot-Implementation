package CalculationExample {
	part vehicle {
		attribute m: ScalarValues::Real;
		
		part eng {
			attribute m: ScalarValues::Real;
		}
		
		part trans {
			attribute m: ScalarValues::Real;
		}
	}
	
	calc def MassSum (
		partMasses: ScalarValues::Real[0..*] )
		totalMass: ScalarValues::Real 
	{
		ScalarFunctions::sum(partMasses)
	}
	
	calc ms: MassSum (
		totalMass = vehicle::m)
		partMasses = {vehicle::eng::m, vehicle::trans::m};
		
	calc c;
}
