package StateTest {
	attribute def Sig {
		x;
	}
	attribute def Exit;
	
	part p;
	
	action act;
	
	state def S {
		do action B;
		entry action A; then S1;
		
		state S1;
			accept s : Sig
			do action D
			then S2;
		
		state S2 {
			do send Sig(T.s.x) to p;
		}
		accept Exit then done;
		
		transition T
			first S2
			accept s : Sig
			if true
			do send s to p
			then S1;
				
		exit act;
	}
	
	state s parallel {
		state s1;
		state s2;
	}
}