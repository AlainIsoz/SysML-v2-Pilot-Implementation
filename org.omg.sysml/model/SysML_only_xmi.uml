      <packagedElement xmi:type="uml:Association" xmi:id="Systems-Actions-A_receiverArgument_sendActionUsage" name="A_receiverArgument_sendActionUsage" visibility="private" memberEnd="Systems-Actions-SendActionUsage-receiverArgument Systems-Actions-A_receiverArgument_sendActionUsage-sendActionUsage">
        <ownedEnd xmi:id="Systems-Actions-A_receiverArgument_sendActionUsage-sendActionUsage" name="sendActionUsage" visibility="public" type="Systems-Actions-SendActionUsage" isDerived="true" association="Systems-Actions-A_receiverArgument_sendActionUsage">
