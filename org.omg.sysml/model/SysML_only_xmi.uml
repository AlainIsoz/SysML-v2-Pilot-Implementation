        <ownedEnd xmi:id="Systems-Views-A_renderingDefinition_definedRendering-definedRendering" name="definedRendering" visibility="public" type="Systems-Views-RenderingUsage" isDerived="true" subsettedProperty="Systems-Parts-A_partDefinition_definedPart-definedPart" association="Systems-Views-A_renderingDefinition_definedRendering">
