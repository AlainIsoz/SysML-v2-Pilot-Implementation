      <packagedElement xmi:type="uml:Association" xmi:id="Systems-UseCases-A_includedUseCase_includingUseCase" name="A_includedUseCase_includingUseCase" visibility="private" memberEnd="Systems-UseCases-UseCaseUsage-includedUseCase Systems-UseCases-A_includedUseCase_includingUseCase-includingUseCase">
        <ownedEnd xmi:id="Systems-UseCases-A_includedUseCase_includingUseCase-includingUseCase" name="includingUseCase" visibility="public" type="Systems-UseCases-UseCaseUsage" isDerived="true" association="Systems-UseCases-A_includedUseCase_includingUseCase">
          <ownedComment xmi:id="Systems-UseCases-A_includedUseCase_includingUseCase-includingUseCase-_ownedComment.0" annotatedElement="Systems-UseCases-A_includedUseCase_includingUseCase-includingUseCase">
            <body>&lt;p>The UseCaseUsage that includes a certain &lt;code>includedUseCase&lt;/code>.&lt;/p></body>
