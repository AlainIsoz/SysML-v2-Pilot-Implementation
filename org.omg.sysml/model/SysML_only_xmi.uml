        <ownedEnd xmi:id="Systems-Constraints-A_assertedConstraint_constraintAssertion-constraintAssertion" name="constraintAssertion" visibility="public" type="Systems-Constraints-AssertConstraintUsage" isDerived="true" association="Systems-Constraints-A_assertedConstraint_constraintAssertion">
