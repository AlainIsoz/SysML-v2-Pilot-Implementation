        </ownedRule>
        <ownedRule xmi:id="Systems-DefinitionAndUsage-Usage-deriveUsageDirectedUsage" name="deriveUsageDirectedUsage" visibility="public" constrainedElement="Systems-DefinitionAndUsage-Usage">
