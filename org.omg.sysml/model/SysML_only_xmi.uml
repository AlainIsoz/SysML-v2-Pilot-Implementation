      <packagedElement xmi:type="uml:Association" xmi:id="Systems-Occurrences-A_eventOccurrence_referencingOccurrence" name="A_eventOccurrence_referencingOccurrence" visibility="private" memberEnd="Systems-Occurrences-EventOccurrenceUsage-eventOccurrence Systems-Occurrences-A_eventOccurrence_referencingOccurrence-referencingOccurrence">
        <ownedEnd xmi:id="Systems-Occurrences-A_eventOccurrence_referencingOccurrence-referencingOccurrence" name="referencingOccurrence" visibility="public" type="Systems-Occurrences-EventOccurrenceUsage" isDerived="true" association="Systems-Occurrences-A_eventOccurrence_referencingOccurrence">
          <ownedComment xmi:id="Systems-Occurrences-A_eventOccurrence_referencingOccurrence-referencingOccurrence-_ownedComment.0" annotatedElement="Systems-Occurrences-A_eventOccurrence_referencingOccurrence-referencingOccurrence">
