          <lowerValue xmi:type="uml:LiteralInteger" xmi:id="Systems-UseCases-A_includedUseCase_includingUseCaseDefinition-includingUseCaseDefinition-_lowerValue" name="" visibility="public"/>
          <upperValue xmi:type="uml:LiteralUnlimitedNatural" xmi:id="Systems-UseCases-A_includedUseCase_includingUseCaseDefinition-includingUseCaseDefinition-_upperValue" name="" visibility="public" value="*"/>
