        <ownedEnd xmi:id="Systems-Views-A_rendering_featuringRenderingDefinition-featuringRenderingDefinition" name="featuringRenderingDefinition" visibility="public" type="Systems-Views-RenderingDefinition" isDerived="true" subsettedProperty="Systems-DefinitionAndUsage-A_usage_featuringDefinition-featuringDefinition" association="Systems-Views-A_rendering_featuringRenderingDefinition">
