          </ownedComment>
        </ownedLiteral>
      </packagedElement>
      <packagedElement xmi:type="uml:Association" xmi:id="Systems-Actions-A_payloadArgument_acceptingActionUsage" name="A_payloadArgument_acceptingActionUsage" visibility="private" memberEnd="Systems-Actions-AcceptActionUsage-payloadArgument Systems-Actions-A_payloadArgument_acceptingActionUsage-acceptingActionUsage">
