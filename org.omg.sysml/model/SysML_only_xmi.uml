      <packagedElement xmi:type="uml:Association" xmi:id="Systems-Views-A_viewDefinition_definedView" name="A_viewDefinition_definedView" visibility="private" memberEnd="Systems-Views-ViewUsage-viewDefinition Systems-Views-A_viewDefinition_definedView-definedView">
