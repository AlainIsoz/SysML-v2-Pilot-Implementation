        <language>OCL2.0</language>
        <body>satisfiedViewpoint = ownedRequirement->
    selectByKind(ViewpointUsage)->
    select(isComposite)</body>
      </specification>
    </ownedRule>
    <ownedRule xmi:id="068576b0-757a-483d-a70f-865160a412cd" name="deriveViewUsageViewCondition" visibility="public" constrainedElement="4b11a587-ab81-4d39-80d1-7e50e8202683">
