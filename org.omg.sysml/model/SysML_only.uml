    <ownedRule xmi:id="ec5a995b-5486-4377-ae6e-67221bbe03c3" name="validateRequirementUsageSubjectParameterPosition" visibility="public" constrainedElement="7248aae3-2934-47cd-bf16-f9aca7f25ad7">
