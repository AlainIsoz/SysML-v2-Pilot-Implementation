    <ownedAttribute xmi:id="5ef4cd7f-9d79-44b5-ad54-30bbd8f1b16b" name="actionDefinition" visibility="public" isOrdered="true" isDerived="true" association="28d7d847-d8df-47dd-b1b5-37c5d6c4cf3e">
