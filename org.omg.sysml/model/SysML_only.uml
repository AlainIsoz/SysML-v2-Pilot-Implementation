    <ownedRule xmi:id="5d75ace0-6226-4ed9-bba6-7f27d058d2e5" name="deriveUseCaseUsageIncludedUseCase" visibility="public" constrainedElement="3fc01c7e-9269-4bfe-8cb6-1a6bca5e2980">
