    </ownedComment>
    <ownedRule xmi:id="64349356-588a-498f-866e-46134290bde4" name="validateRequirementVerificationMembershipKind" visibility="public" constrainedElement="bbe6f298-3ad5-4b8d-86a7-0c500ad29d37">
