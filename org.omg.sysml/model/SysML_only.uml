    </ownedRule>
    <ownedRule xmi:id="333a6749-3d82-4e3c-acb8-75de5255e237" name="validateRequirementVerificationMembershipOwningType" visibility="public" constrainedElement="bbe6f298-3ad5-4b8d-86a7-0c500ad29d37">
