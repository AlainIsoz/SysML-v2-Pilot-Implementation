    <ownedRule xmi:id="f0209f3e-f508-4c54-8c5e-c10a4efa0072" name="deriveCaseDefinitionObjectiveRequirement" visibility="public" constrainedElement="20e23891-303c-400f-909a-032f49ef946d">
