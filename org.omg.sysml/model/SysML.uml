    <ownedRule xmi:id="6c4299f6-087d-4ab0-8b67-465539b403ee" name="validateForkNodeIncomingSuccessions" visibility="public" constrainedElement="1bbf6e74-3284-45c3-81b4-82431ea8bf16">
