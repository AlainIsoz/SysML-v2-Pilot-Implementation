    </ownedRule>
    <ownedRule xmi:id="315fecfa-8c2c-4398-a584-5474ccacf758" name="deriveNamespaceOwnedImport" visibility="public" constrainedElement="732bd40e-fb64-42e2-8663-8fcee1dac982">
