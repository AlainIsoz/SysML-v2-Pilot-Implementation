        <body>isModelLevelEvaluable = modelLevelEvaluable(Set(Element){})</body>
      </specification>
    </ownedRule>
    <ownedRule xmi:id="130f63a9-9976-4388-9272-658394a1b036" name="checkExpressionSpecialization" visibility="public" constrainedElement="b32c4dd3-88a2-4418-b414-e75ceb0bb432">
