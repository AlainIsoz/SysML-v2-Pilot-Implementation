      <upperValue xmi:type="uml:LiteralUnlimitedNatural" xmi:id="116f40d4-0e10-4952-aca7-9d2a6316f1c7" name="" visibility="public" value="1"/>
    </ownedAttribute>
    <ownedOperation xmi:id="1901a006-28ec-499e-9ae4-e33b5d725284" name="importedMemberships" visibility="public" bodyCondition="d692bc99-5738-4718-822b-c74e781627b3" redefinedOperation="68b7f828-1f54-4bbd-81ad-99523b0c3b44">
      <ownedComment xmi:id="a6736bb3-e309-4609-ae8e-15cb07cb1f4a" annotatedElement="1901a006-28ec-499e-9ae4-e33b5d725284">
        <body>&lt;p>Returns at least the &lt;code>importedMembership&lt;/code>. If &lt;code>isRecursive = true&lt;/code> and the &lt;code>memberElement&lt;/code> of the &lt;code>importedMembership&lt;/code> is a &lt;code>Namespace&lt;/code>, then &lt;code>Memberships&lt;/code> are also recursively imported from that &lt;code>Namespace&lt;/code>.&lt;/p>
</body>
      </ownedComment>
      <ownedRule xmi:id="d692bc99-5738-4718-822b-c74e781627b3" name="unnamed1" visibility="public">
        <specification xmi:type="uml:OpaqueExpression" xmi:id="5d4d9eac-e8af-4939-a60a-9b67cacb3480" name="" visibility="public">
          <language>OCL2.0</language>
          <body>if not isRecursive or 
   not importedElement.oclIsKindOf(Namespace) or
   excluded->includes(importedElement)
then Sequence{importedMembership}
else importedElement.oclAsType(Namespace).
        visibleMemberships(excluded, true, importAll)->
        prepend(importedMembership)
endif</body>
        </specification>
      </ownedRule>
      <ownedParameter xmi:id="d1f68c1a-5bf7-46b8-afdf-4df33e49c787" name="excluded" visibility="public" type="732bd40e-fb64-42e2-8663-8fcee1dac982">
