    <ownedRule xmi:id="bfc0404b-fd68-4f5a-b705-3e59268de7eb" name="checkStepOwnedPerformanceSpecialization" visibility="public" constrainedElement="b1a2875a-af89-47c5-ad2b-d5763c43aafd">
