    </ownedRule>
    <ownedRule xmi:id="c4228a59-62f3-4ea1-9f3b-374d064444d2" name="validateDefinitionVariationSpecialization" visibility="public" constrainedElement="bbec763b-2ef0-415f-9e22-c531739690bb">
