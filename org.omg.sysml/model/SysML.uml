  <packagedElement xmi:type="uml:Association" xmi:id="b90750f5-e0a2-4cad-8f03-3f1353fde024" name="" visibility="public" memberEnd="45afc642-b76b-4cb9-9e5c-dea8f15ffe41 3c0a5f74-5ec1-4027-97f1-bfdbe97b0c21">
