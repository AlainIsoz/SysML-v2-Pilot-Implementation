    not ownedSpecialization.specific->exists(isVariation)</body>
      </specification>
    </ownedRule>
    <ownedRule xmi:id="42c9524d-b98d-4221-9013-12cf1e2c722f" name="checkUsageVariationDefinitionSpecialization" visibility="public" constrainedElement="46068009-84f1-4001-a9bb-fc03fd6a381c">
