        <language>OCL2.0</language>
        <body>specializesFromLibrary(&quot;Occurrences::Life&quot;)</body>
      </specification>
    </ownedRule>
    <ownedRule xmi:id="a00420ab-bf66-4fce-a77b-0cb72d14d1ca" name="checkLifeClassMultiplicitySpecialization" visibility="public" constrainedElement="7f1b57c3-ab17-4a52-a545-08b19c24a5d0">
