    <ownedRule xmi:id="360ee380-03bf-47db-a341-473d7f5158c4" name="validateUsageVariationSpecialization" visibility="public" constrainedElement="46068009-84f1-4001-a9bb-fc03fd6a381c">
      <ownedComment xmi:id="21a18990-466a-4b53-b15e-49854bdf3b22" annotatedElement="360ee380-03bf-47db-a341-473d7f5158c4">
        <body>&lt;p>A variation &lt;code>Usage&lt;/code> may not specialize any variation &lt;code>Definition&lt;/code> or &lt;code>Usage&lt;/code>.&lt;/p></body>
      </ownedComment>
      <specification xmi:type="uml:OpaqueExpression" xmi:id="ba10e136-3745-4a7b-bda9-1e599f8f8f2f" name="" visibility="public">
        <language>OCL2.0</language>
