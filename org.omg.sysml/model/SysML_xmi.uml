      <packagedElement xmi:type="uml:Association" xmi:id="Systems-DefinitionAndUsage-A_nestedPort_portOwningUsage" name="A_nestedPort_portOwningUsage" visibility="private" memberEnd="Systems-DefinitionAndUsage-Usage-nestedPort Systems-DefinitionAndUsage-A_nestedPort_portOwningUsage-portOwningUsage">
        <ownedEnd xmi:id="Systems-DefinitionAndUsage-A_nestedPort_portOwningUsage-portOwningUsage" name="portOwningUsage" visibility="public" type="Systems-DefinitionAndUsage-Usage" isDerived="true" redefinedProperty="Systems-DefinitionAndUsage-Usage-owningUsage" association="Systems-DefinitionAndUsage-A_nestedPort_portOwningUsage">
          <ownedComment xmi:id="Systems-DefinitionAndUsage-A_nestedPort_portOwningUsage-portOwningUsage-_ownedComment.0" annotatedElement="Systems-DefinitionAndUsage-A_nestedPort_portOwningUsage-portOwningUsage">
