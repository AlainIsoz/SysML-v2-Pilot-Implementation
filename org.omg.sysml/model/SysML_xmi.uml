          <lowerValue xmi:type="uml:LiteralInteger" xmi:id="Kernel-Expressions-A_argument_invocation-invocation-_lowerValue" name="" visibility="public"/>
          <upperValue xmi:type="uml:LiteralUnlimitedNatural" xmi:id="Kernel-Expressions-A_argument_invocation-invocation-_upperValue" name="" visibility="public" value="1"/>
        </ownedEnd>
