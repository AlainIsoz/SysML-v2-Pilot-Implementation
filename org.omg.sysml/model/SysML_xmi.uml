      <packagedElement xmi:type="uml:Association" xmi:id="Kernel-Behaviors-A_step_featuringBehavior" name="A_step_featuringBehavior" visibility="private" memberEnd="Kernel-Behaviors-Behavior-step Kernel-Behaviors-A_step_featuringBehavior-featuringBehavior">
        <ownedEnd xmi:id="Kernel-Behaviors-A_step_featuringBehavior-featuringBehavior" name="featuringBehavior" visibility="public" type="Kernel-Behaviors-Behavior" isDerived="true" subsettedProperty="Core-Types-A_typeWithFeature_feature-typeWithFeature" association="Kernel-Behaviors-A_step_featuringBehavior">
