      <packagedElement xmi:type="uml:Association" xmi:id="Systems-DefinitionAndUsage-A_usage_featuringDefinition" name="A_usage_featuringDefinition" visibility="private" memberEnd="Systems-DefinitionAndUsage-Definition-usage Systems-DefinitionAndUsage-A_usage_featuringDefinition-featuringDefinition">
