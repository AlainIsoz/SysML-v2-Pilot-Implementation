      <packagedElement xmi:type="uml:Association" xmi:id="Systems-Views-A_ownedRendering_viewRenderingMembership" name="A_ownedRendering_viewRenderingMembership" visibility="private" memberEnd="Systems-Views-ViewRenderingMembership-ownedRendering Systems-Views-A_ownedRendering_viewRenderingMembership-viewRenderingMembership">
        <ownedEnd xmi:id="Systems-Views-A_ownedRendering_viewRenderingMembership-viewRenderingMembership" name="viewRenderingMembership" visibility="public" type="Systems-Views-ViewRenderingMembership" isDerived="true" association="Systems-Views-A_ownedRendering_viewRenderingMembership">
