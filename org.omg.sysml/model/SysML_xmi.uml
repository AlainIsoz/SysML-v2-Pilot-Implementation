        <ownedEnd xmi:id="Kernel-Behaviors-A_parameter_parameteredStep-parameteredStep" name="parameteredStep" visibility="public" type="Kernel-Behaviors-Step" isDerived="true" subsettedProperty="Core-Types-A_typeWithFeature_feature-typeWithFeature" association="Kernel-Behaviors-A_parameter_parameteredStep">
