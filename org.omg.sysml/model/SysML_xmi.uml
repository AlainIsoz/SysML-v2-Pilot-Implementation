      <packagedElement xmi:type="uml:Association" xmi:id="Core-Features-A_owningFeature_ownedSubsetting" name="A_owningFeature_ownedSubsetting" visibility="private" memberEnd="Core-Features-Subsetting-owningFeature Core-Features-Feature-ownedSubsetting"/>
      <packagedElement xmi:type="uml:Association" xmi:id="Core-Features-A_subsettedFeature_supersetting" name="A_subsettedFeature_supersetting" visibility="private" memberEnd="Core-Features-Subsetting-subsettedFeature Core-Features-A_subsettedFeature_supersetting-supersetting">
        <ownedEnd xmi:id="Core-Features-A_subsettedFeature_supersetting-supersetting" name="supersetting" visibility="public" type="Core-Features-Subsetting" subsettedProperty="Core-Types-A_general_generalization-generalization" association="Core-Features-A_subsettedFeature_supersetting">
          <ownedComment xmi:id="Core-Features-A_subsettedFeature_supersetting-supersetting-_ownedComment.0" annotatedElement="Core-Features-A_subsettedFeature_supersetting-supersetting">
            <body>&lt;p>The Subsettings with a certain Feature as the &lt;code>subsettedFeature&lt;/code>.&lt;/p></body>
