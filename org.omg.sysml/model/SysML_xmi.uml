        <ownedEnd xmi:id="Kernel-Behaviors-A_parameter_parameteredBehavior-parameteredBehavior" name="parameteredBehavior" visibility="public" type="Kernel-Behaviors-Behavior" isDerived="true" subsettedProperty="Core-Types-A_typeWithFeature_feature-typeWithFeature" association="Kernel-Behaviors-A_parameter_parameteredBehavior">
          <lowerValue xmi:type="uml:LiteralInteger" xmi:id="Kernel-Behaviors-A_parameter_parameteredBehavior-parameteredBehavior-_lowerValue" name="" visibility="public"/>
          <upperValue xmi:type="uml:LiteralUnlimitedNatural" xmi:id="Kernel-Behaviors-A_parameter_parameteredBehavior-parameteredBehavior-_upperValue" name="" visibility="public" value="*"/>
