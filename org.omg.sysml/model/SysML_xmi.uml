          </ownedComment>
          <lowerValue xmi:type="uml:LiteralInteger" xmi:id="Systems-DefinitionAndUsage-A_nestedPort_portOwningUsage-portOwningUsage-_lowerValue" name="" visibility="public"/>
          <upperValue xmi:type="uml:LiteralUnlimitedNatural" xmi:id="Systems-DefinitionAndUsage-A_nestedPort_portOwningUsage-portOwningUsage-_upperValue" name="" visibility="public" value="1"/>
        </ownedEnd>
