          </ownedComment>
          <specification xmi:type="uml:OpaqueExpression" xmi:id="Systems-Actions-ForLoopActionUsage-validateForLoopActionUsageParameters-_specification" name="" visibility="public">
            <language>OCL2.0</language>
            <body>inputParameters()->size() = 2</body>
          </specification>
        </ownedRule>
        <generalization xmi:id="Systems-Actions-ForLoopActionUsage-_generalization.0" general="Systems-Actions-LoopActionUsage"/>
