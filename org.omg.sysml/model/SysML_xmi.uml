        <ownedEnd xmi:id="Systems-Occurrences-A_individualDefinition_individualUsage-individualUsage" name="individualUsage" visibility="public" type="Systems-Occurrences-OccurrenceUsage" isDerived="true" association="Systems-Occurrences-A_individualDefinition_individualUsage">
